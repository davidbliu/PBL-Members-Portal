
<table class = 'table table-condensed table-bordered'>
	<thead>
		<th>Key</th>
		<th>Name</th>
		<th>Committee</th>
		<th>Timestamp</th>
	</thead>
	<tbody>
		<% @recent.each do |click| %>
		<tr>
			<td><%= click.key %></td>
			<td>
				<%= @email_hash.keys.include?(click.member_email) ? @email_hash[click.member_email].name : click.member_email %></td>
			<td>
				<%= @email_hash.keys.include?(click.member_email) ? @email_hash[click.member_email].committee : '' %></td>
			</td>
			<td>
				<%= click.time_string %>
			</td>
		</tr>
		<% end %>
	</tbody>
</table>
	